    "lodash.merge": "^4.6.2",
    "tailwindcss": "^2.1.1",
    "vite": "^2.1.0",
    "vitepress": "^0.12.2"
  },
  "files": [
    "dist",
    "tailwind.config.js",
    "src/assets/css/index.css",
    "tailwind/plugins/typography.js"
  ],
  "main": "./dist/repair-components.umd.js",
  "module": "./dist/repair-components.es.js",
  "exports": {
    ".": {
